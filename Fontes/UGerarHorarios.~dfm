  object SQLHorario: TSQLDataSet
    CommandText = 
      'SELECT'#13#10#13#10'HORARIOS.IDHOR,'#13#10'HORARIOS.IDRELACAO,'#13#10'HORARIOS.DIA_HOR' +
      ','#13#10'HORARIOS.AULA_HOR,'#13#10'HORARIOS.TURNO_HOR,'#13#10'HORARIOS.STATUS_HOR,' +
      #13#10'HORARIOS.IDTURMA,'#13#10#13#10#13#10#13#10'TURMAS.NUM_TUR,'#13#10'DISCURSO.IDDISC,'#13#10'DI' +
      'SCPROF.IDFUNC,'#13#10'DISCIPLINAS.DESCR_DISC,'#13#10#13#10'FUNCIONARIO.NOME_FUNC' +
      #13#10#13#10'FROM HORARIOS'#13#10#13#10'LEFT OUTER JOIN DISCPROF ON (DISCPROF.IDREL' +
      'ACAO = HORARIOS.IDRELACAO)'#13#10#13#10'LEFT OUTER JOIN DISCURSO ON ( DISC' +
      'URSO.IDDISC=DISCPROF.IDDISCUR)'#13#10#13#10'LEFT OUTER JOIN CURSOS ON ( CU' +
      'RSOS.IDCURSO=DISCURSO.IDDISCUR)'#13#10#13#10#13#10#13#10#13#10'LEFT OUTER JOIN DISCIPL' +
      'INAS ON ( DISCIPLINAS.IDDISC=DISCURSO.IDDISC)'#13#10#13#10'LEFT OUTER JOIN' +
      ' FUNCIONARIO ON (FUNCIONARIO.IDFUNC = DISCPROF.IDFUNC)'#13#10#13#10'LEFT O' +
      'UTER JOIN TURMAS ON (TURMAS.IDTUR = HORARIOS.IDTURMA)'#13#10#13#10#13#10#13#10#13#10#13 +
      #10#13#10
