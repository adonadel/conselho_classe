  object SQLConselho: TSQLDataSet
    CommandText = 
      'SELECT'#13#10#13#10'CONSELHO.IDCONS,'#13#10'CONSELHO.IDTUR,'#13#10'CONSELHO.DATA_CONS,' +
      #13#10'CONSELHO.HORA_CONS,'#13#10'CONSELHO.NUMBIMTRIM_CONS,'#13#10'CONSELHO.TIPOC' +
      'ONS_CONS,'#13#10'CONSELHO.OBS_CONS,'#13#10'CONSELHO.STATUS_CONS,'#13#10#13#10'TURMAS.A' +
      'NO_TUR,'#13#10'TURMAS.TURNO_TUR,'#13#10'TURMAS.STATUS_TUR'#13#10#13#10'FROM CONSELHO'#13#10 +
      #13#10'LEFT OUTER JOIN TURMAS ON (TURMAS.IDTUR = CONSELHO.IDTUR)'#13#10#13#10'W' +
      'HERE TURMAS.ANO_TUR        = :PARANO AND'#13#10'               TURMAS.' +
      'TURNO_TUR  = :PARTURNO AND'#13#10'               TURMAS.STATUS_TUR = :' +
      'PARSTATUS AND'#13#10'               CONSELHO.NUMBIMTRIM_CONS = :PARBIM' +
      ' '
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'PARANO'
        ParamType = ptInput
        Value = 0
      end
      item
        DataType = ftString
